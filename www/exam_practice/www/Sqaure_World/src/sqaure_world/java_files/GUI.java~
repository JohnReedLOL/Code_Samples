            jLabel_X_1_Y_n1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 1][2 - 1].name)));
            jLabel_X_2_Y_n1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 2][2 - 1].name)));
