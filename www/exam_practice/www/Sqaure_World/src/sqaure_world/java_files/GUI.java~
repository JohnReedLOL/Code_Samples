    //vlad please adf arrow keys. Thank you!
    private void formKeyPressed(java.awt.event.KeyEvent evt) {
        //System.out.println("A key was pressed");
        if (evt.getKeyCode() == 38) {
            //System.out.println("up was pressed");
            Move_Object m = new Move_Object(0, 1);
            output_thread = new Thread(new Clients_Output(m));
            output_thread.start();
        } // Up
        else if (evt.getKeyCode() == 40) {
            //System.out.println("down was pressed");
            Move_Object m = new Move_Object(0, -1);
            output_thread = new Thread(new Clients_Output(m));
            output_thread.start();
        }// Left
        else if (evt.getKeyCode() == 37) {
            //System.out.println("left was pressed");
            Move_Object m = new Move_Object(-1, 0);
            output_thread = new Thread(new Clients_Output(m));
            output_thread.start();
        }// Left
        else if (evt.getKeyCode() == 39) {
            //System.out.println("right was pressed");
            Move_Object m = new Move_Object(1, 0);
            output_thread = new Thread(new Clients_Output(m));
            output_thread.start();
        }// Left

    }

    private void jTextField1_Outgoing_LineKeyPressed(java.awt.event.KeyEvent evt)//GEN-FIRST:event_jTextField1_Outgoing_LineKeyPressed
    {//GEN-HEADEREND:event_jTextField1_Outgoing_LineKeyPressed
        // TODO add your handling code here:
        if (evt.getKeyCode() == KeyEvent.VK_ENTER) // Enter key pressed
        {
            //System.out.println("Enter key was pressed.");
            String to_send = jTextField1_Outgoing_Line.getText() + "\n";
            //System.out.println(":" + to_send + ":");
            //try {
            //Square_World_GUI.oos.writeObject(to_send);
            output_thread = new Thread(new Clients_Output(to_send));
            output_thread.start();
            if (to_send.contentEquals("logout" + "\n")) {
                System.exit(1);
            }

            //}
            //catch (IOException ee) {
            //    //System.out.println("Fatal exception in ");
            //    System.exit(1);
            //}
            jTextField1_Outgoing_Line.setText(""); // Resets text to empty.
            //textArea.append(text + newline);
            //textField.selectAll();
            //jTextField_Outgoing.setCa
            //Make sure the new text is visible, even if there
            //was a selection in the text area.
            //textArea.setCaretPosition(textArea.getDocument().getLength());
        }

        //Thread string_outputter = new Thread(new Clients_Output(evt));
    }//GEN-LAST:event_jTextField1_Outgoing_LineKeyPressed

