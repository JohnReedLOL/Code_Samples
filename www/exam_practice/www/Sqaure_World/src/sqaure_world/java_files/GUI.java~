            jLabel_X_n4_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 4][2].name)));
            jLabel_X_n3_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 3][2].name)));
            jLabel_X_n2_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 2][2].name)));
            jLabel_X_n1_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 1][2].name)));
            jLabel_X_0_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 0][2].name)));
            jLabel_X_1_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 1][2].name)));
            jLabel_X_2_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 2][2].name)));
            jLabel_X_3_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 3][2].name)));
            jLabel_X_4_Y_0.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 4][2].name)));

            jLabel_X_n4_Y_1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 4][2 + 1].name)));
            jLabel_X_n3_Y_1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 3][2 + 1].name)));
            jLabel_X_n2_Y_1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 2][2 + 1].name)));
