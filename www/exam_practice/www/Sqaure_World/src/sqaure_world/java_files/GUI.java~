    public void update_The_Map_Desplay() {

        try {
            //System.out.println("NAME IS:" + yourView.tilesInView[4 - 4][2 - 2].name);
            //System.out.println("BF pointer is: " + Main.image_repo.images.get(yourView.tilesInView[4 - 4][2 - 2].name));

            jLabel_X_n4_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 4][2 - 2].name)));
            jLabel_X_n3_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 3][2 - 2].name)));
            jLabel_X_n2_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 2][2 - 2].name)));
            jLabel_X_n1_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 1][2 - 2].name)));
            jLabel_X_0_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 0][2 - 2].name)));
            jLabel_X_1_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 1][2 - 2].name)));
            jLabel_X_2_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 2][2 - 2].name)));
            jLabel_X_3_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 3][2 - 2].name)));
            jLabel_X_4_Y_n2.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 + 4][2 - 2].name)));

            jLabel_X_n4_Y_n1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 4][2 - 1].name)));
            jLabel_X_n3_Y_n1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 3][2 - 1].name)));
            jLabel_X_n2_Y_n1.setIcon(new ImageIcon(Main.image_repo.images.get(yourView.tilesInView[4 - 2][2 - 1].name)));
